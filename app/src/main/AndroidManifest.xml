<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lifecoachingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LifeCoachingApp">

        <activity android:name=".Profile_page"></activity>

        <activity
            android:name=".TestChapterTree"
            android:parentActivityName="com.example.lifecoachingapp.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.lifecoachingapp.MainActivity" />
        </activity>

        <activity
            android:name=".Discovery"
            android:label="@string/title_discovery"
            />
        <activity
            android:name=".Profile"
            android:label="@string/title_profile"
            />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>